module Lune.String exposing (append, eq, fromInt, fromFloat, toInt)

{-| Surface API per `spec/lune_standard_library_v0_1.md`. -}

import Lune.Prelude exposing (Bool(..), Int, Float, Result(..), String)

append : String -> String -> String
append = prim_appendString

eq : String -> String -> Bool
eq = prim_eqString

fromInt : Int -> String
fromInt = prim_showInt

fromFloat : Float -> String
fromFloat = prim_showFloat

toInt : String -> Result String Int
toInt = prim_parseInt
