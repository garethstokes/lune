module InstanceDemo exposing (idList)

import Lune.Prelude exposing (Functor(..), Int)

type MyList a =
  MyNil
  | MyCons a (MyList a)

instance Functor MyList where
  map = \f xs ->
    case xs of
      MyNil -> MyNil
      MyCons x xt -> MyCons (f x) (map f xt)

idList : MyList Int
idList =
  map (\x -> x) (MyCons 1 MyNil)
