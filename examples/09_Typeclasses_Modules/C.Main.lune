module C.Main exposing (main)

import A.Show exposing (Show(..))
import B.ShowInt
import Lune.IO
import Lune.Int
import Lune.Prelude exposing (IO, Unit)

printTwice : Show a => a -> IO Unit
printTwice x =
  do
    IO.println (show x)
    IO.println (show x)

main : IO Unit
main =
  printTwice (Int.add 1 2)

